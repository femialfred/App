<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.femialfred.agileapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_agile_logo"
        android:label="@string/AgileApp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".AgiPedia1" />
        <activity android:name=".AgiPedia2" />
        <activity android:name=".AgiPedia3" />
        <activity android:name=".AgiPedia4" />
        <activity android:name=".AgiPedia5" />
        <activity android:name=".AgiPedia6" />
        <activity android:name=".AgiPedia7" />
        <activity android:name=".AgiPedia8" />
        <activity android:name=".AgiPedia9" />
        <activity android:name=".AgiPedia10">
        </activity>
    </application>

</manifest>